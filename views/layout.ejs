    <script src="/js/libs/marked.min.js"></script>
    <script src="/js/libs/tooltip.js"></script>
    <script src="/js/pages/collaborator.js"></script>
    <script src="/js/pages/examples.js"></script>
    <script src="/js/pages/package.js"></script>
    <script src="/js/pages/search.js"></script>
    <script src="/js/pages/source.js"></script>
    <script src="/js/pages/task_views.js"></script>
    <script src="/js/pages/trending.js"></script>
